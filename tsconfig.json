{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/db": [
        "libs/db/src"
      ],
      "@app/db/*": [
        "libs/db/src/*"
      ],
      "@app/lesson-material": [
        "libs/lesson-material/src"
      ],
      "@app/lesson-material/*": [
        "libs/lesson-material/src/*"
      ],
      "@app/parameters": [
        "libs/parameters/src"
      ],
      "@app/parameters/*": [
        "libs/parameters/src/*"
      ],
      "@app/shared": [
        "libs/shared/src"
      ],
      "@app/shared/*": [
        "libs/shared/src/*"
      ],
      "@app/typeorm": [
        "libs/typeorm/src"
      ],
      "@app/typeorm/*": [
        "libs/typeorm/src/*"
      ],
      "@app/user": [
        "libs/user/src"
      ],
      "@app/user/*": [
        "libs/user/src/*"
      ],
      "@app/utili": [
        "libs/utili/src"
      ],
      "@app/utili/*": [
        "libs/utili/src/*"
      ]
    }
  }
}